{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:DescribeLogStreams",
            "logs:GetLogEvents",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:Batch*",
            "dynamodb:DeleteItem",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "kms:Decrypt"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:dynamodb:eu-central-1:196241772369:table/cmtr-d49b0e2c-Configuration",
            "arn:aws:dynamodb:eu-central-1:196241772369:table/cmtr-d49b0e2c-Audit",
            "arn:aws:logs:*:*:*"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy",
    "tags": {}
  },
  "Configuration": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "key",
    "hash_key_type": "S",
    "read_capacity": 5,
    "write_capacity": 5,
    "global_indexes": [],
    "autoscaling": [],
    "stream_specification": {
      "stream_enabled": true,
      "stream_view_type": "NEW_AND_OLD_IMAGES"
    }
  },
  "Audit": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "id",
    "hash_key_type": "S",
    "read_capacity": 5,
    "write_capacity": 5,
    "global_indexes": [],
    "autoscaling": []
  }
}
